  <select id="findCoinsetPage"   resultType="com.house730.admin.pojo.SysPropertyCoinset">
  		select 
			*
		from sys_propertycoinset s
		where s.IsDelete = 0
			<if test="isshow != null and isshow != ''">
				AND s.IsShow = #{isshow,jdbcType=BIT} 
			</if>  
			<if test="type != null and type != ''">
				AND s.Type = #{type,jdbcType=INTEGER} 
			</if>  
		ORDER BY s.CreateTime desc
  	</select>
  
  <select id="findCoinsetBySetId"   resultType="com.house730.admin.pojo.SysPropertyCoinset">
